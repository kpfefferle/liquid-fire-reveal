{
  "name": "liquid-fire-reveal",
  "version": "0.5.0",
  "description": "Reveal transitions for liquid-fire.",
  "keywords": [
    "ember-addon",
    "liquid-fire",
    "transitions"
  ],
  "repository": "https://github.com/kpfefferle/liquid-fire-reveal",
  "license": "MIT",
  "author": "Kevin Pfefferle",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.1.0"
  },
  "devDependencies": {
    "@ember/jquery": "^2.0.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^2.0.2",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.6.0",
    "ember-auto-import": "^2.5.0",
    "ember-cli": "~4.9.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "2.0.0",
    "ember-cli-deploy-build": "2.0.0",
    "ember-cli-deprecation-workflow": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^9.0.1",
    "ember-source": "~4.10.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.2.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.2.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.3.4",
    "liquid-fire": "0.34.0",
    "loader.js": "^4.7.0",
    "normalize.css": "^8.0.0",
    "prettier": "^2.8.1",
    "qunit": "^2.19.3",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "resolutions": {
    "@glimmer/interfaces": "^0.84.2",
    "@glimmer/reference": "^0.84.2"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://kevin.pfefferle.co/liquid-fire-reveal/"
  },
  "volta": {
    "node": "16.13.2",
    "yarn": "1.22.18"
  }
}
