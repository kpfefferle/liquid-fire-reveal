{
  "name": "liquid-fire-reveal",
  "version": "0.5.0",
  "description": "Reveal transitions for liquid-fire.",
  "keywords": [
    "ember-addon",
    "liquid-fire",
    "transitions"
  ],
  "repository": "https://github.com/kpfefferle/liquid-fire-reveal",
  "license": "MIT",
  "author": "Kevin Pfefferle",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@ember/jquery": "^2.0.0",
    "@ember/optional-features": "^2.1.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^4.0.2",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.0.0",
    "ember-auto-import": "^2.7.4",
    "ember-cli": "~6.0.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deploy": "2.0.0",
    "ember-cli-deploy-build": "3.0.0",
    "ember-cli-deprecation-workflow": "^3.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^8.1.0",
    "ember-resolver": "^13.0.0",
    "ember-source": "~6.0.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.0.0",
    "ember-try": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.0.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qunit": "^8.1.1",
    "liquid-fire": "0.37.0",
    "loader.js": "^4.7.0",
    "normalize.css": "^8.0.0",
    "prettier": "^3.3.3",
    "qunit": "^2.22.0",
    "qunit-dom": "^3.0.0",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "webpack": "^5.93.0"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0"
  },
  "resolutions": {
    "@glimmer/interfaces": "^0.84.2",
    "@glimmer/reference": "^0.84.2"
  },
  "engines": {
    "node": "18.* || >= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://kevin.pfefferle.co/liquid-fire-reveal/"
  },
  "volta": {
    "node": "18.19.1",
    "yarn": "1.22.22"
  }
}
